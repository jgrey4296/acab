// TODO basic equivalencies in semantics. eg: a, an.
an.import.rule:
    #importer
    ~module.math(::python.module)?
    ᛦ $x(λmentions add(::operator))?

    λimport module.math                -> $math(::python.module)
    λimport.extract $math add          -> $add
    λtrie.flatten $math $x(::operator) -> $flattened

    λctx.bind "+" $add
    + module.$math
    λctx.bind.tag #math $math
end
