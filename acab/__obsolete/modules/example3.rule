acab.numbers.abstract:
    curve.sin
    curve.pow
    distribution.$x
    delta.$y

    λsample $x normal -> $y

end

acab.numbers.actual:
    a.val.1
    a.val.2
    a.decimal.4d553

    [(2 + $x) ^ 3] -> $y
end

acab.probability:
    val.[1/5](::probability)?
    $x(λdraw 2)?

    λprob.and $x $y -> $z
    λprob.or $x $y -> $z

    λdraw $x -> $y
    λdraw.replace $x -> $y

end

acab.value.sentence:
    a.b.$x(::Sentence)?
    $x(λ∩ example.words)?

    an.example.[test.sentence]

end
